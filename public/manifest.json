{
  "manifest_version": 3,
  "name": "Expense Lens",
  "version": "0.1.0",
  "description": "Sum prices on any page using a CSS selector. Nothing leaves your browser.",
  "action": {
    "default_title": "Expense Lens",
    "default_popup": "index.html",
    "default_icon": {
      "16":  "icons/extension-lens-16.png",
      "24":  "icons/extension-lens-24.png",
      "32":  "icons/extension-lens-32.png",
      "38":  "icons/extension-lens-38.png",
      "48":  "icons/extension-lens-48.png"
    }
  },
  "icons": {
    "16":  "icons/extension-lens-16.png",
    "48":  "icons/extension-lens-48.png",
    "128": "icons/extension-lens-128.png"
  },
  "permissions": ["activeTab", "scripting", "storage"]
}
